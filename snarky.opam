opam-version: "1.2"
name: "snarky"
maintainer: "opensource@o1labs.org"
authors: ["O(1) Labs, LLC <opensource@o1labs.org>"]
homepage: "https://github.com/o1labs/snarky"
bug-reports: "https://github.com/o1labs/snarky/issues"
dev-repo: "git+https://github.com/o1labs/snarky.git"
license: "MIT"
build: [
  ["dune" "build" "-p" name "-j" jobs]
]
depends: [
  "core_kernel" {>= "0.12.0" & < "0.13.0" }
  "fold_lib"
  "tuple_lib"
  "bitstring_lib"
  "interval_union"
  "bignum"
  "ctypes" {>= "0.14.0" & < "0.15.0"}
  "ctypes-foreign" {>= "0.4.0" & < "0.5.0"}
  "ppx_snarky"
  "ppx_jane" {>= "0.12.0" & < "0.13.0"}
  "ppx_deriving" {>= "4.2.1" & < "4.3.0"}
  "bisect_ppx" {>= "1.4.1" & < "1.5.0"}
  "dune"                {build & >= "1.0"}
]
available: [ ocaml-version >= "4.04.1" ]
descr: "
A snarks DSL
"

